{
    "name": "spatie/laravel-webhook-client",
    "description": "Receive webhoops in Laravel apps",
    "keywords": [
        "spatie",
        "laravel-webhook-client"
    ],
    "homepage": "https://github.com/spatie/laravel-webhook-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.6",
        "illuminate/bus": "^5.4|^5.8|^6.0|^7.0",
        "illuminate/database": "^5.4|^5.8|^6.0|^7.0",
        "illuminate/support": "^5.4|^5.8|^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "v3.4.0",
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\WebhookClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\WebhookClient\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "5.6.4"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\WebhookClient\\WebhookClientServiceProvider"
            ]
        }
    }
}

